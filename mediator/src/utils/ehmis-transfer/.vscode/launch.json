{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Get Patient Profile Test",
        "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
        "args": [
          "--no-timeouts",
          "--colors",
          "${workspaceFolder}/tests/get-patient-profile.spec.js"
        ],
        "internalConsoleOptions": "openOnSessionStart",
        "outFiles": [
          "${workspaceFolder}/dist/**/*.js"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  